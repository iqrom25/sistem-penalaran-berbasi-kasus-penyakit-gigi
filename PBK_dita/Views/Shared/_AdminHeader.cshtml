
      <header class="header header-sticky mb-4">
        <div class="container-fluid">
          <button class="header-toggler px-md-0 me-md-3" type="button" onclick="coreui.Sidebar.getInstance(document.querySelector('#sidebar')).toggle()">
            <i class="icon icon-lg bi bi-list">
              </i>
          </button>
         <ul class="header-nav d-none d-md-flex">
                     <li class="nav-item"><a target="_blank" class="nav-link" asp-controller="Home" asp-action="Index"><i class="bi bi-house-door me-1"></i>Home Page</a></li>

                   </ul>
          <ul class="header-nav ms-3">
            <li class="nav-item dropdown"><a class="nav-link py-0" data-coreui-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                <div class="avatar avatar-md">
                  <img class="avatar-img" src="~/img/img-profile.jpg" alt="admin">
                </div>
              </a>
              <div class="dropdown-menu dropdown-menu-end pt-0">
                  <a class="dropdown-item" asp-action="Logout" asp-controller="Auth">
                  <i class="icon me-2 bi bi-box-arrow-left">
                  </i> Logout</a>
              </div>
            </li>
          </ul>
        </div>
        <div class="header-divider"></div>
        <div class="container">
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb my-0 ms-2">
              <li class="breadcrumb-item">
                <!-- if breadcrumb is single--><span>@ViewData["controller"]</span>
              </li>
              <li class="breadcrumb-item active"><span>@ViewData["action"]</span></li>
              @if (ViewData["extra"] is not null)
              {
                <li class="breadcrumb-item active"><span>@ViewData["extra"]</span></li>
              }
            </ol>
          </nav>
        </div>
      </header>